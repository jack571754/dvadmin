{
  "project_name": "django-vue3-admin",
  "description": "管理系统开发框架 - 基于 RBAC 模型的权限管理系统，支持列级别权限控制",
  "last_updated": "2026-01-23T14:19:21+08:00",
  "version": "3.2.0",
  "repository": {
    "gitee": "https://gitee.com/huge-dream/django-vue3-admin",
    "github": "https://github.com/huge-dream/django-vue3-admin",
    "website": "https://www.django-vue-admin.com",
    "demo": "https://demo.dvadmin.com"
  },
  "tech_stack": {
    "backend": {
      "language": "Python",
      "framework": "Django 4.2.14",
      "api_framework": "Django REST Framework 3.15.2",
      "auth": "djangorestframework_simplejwt 5.4.0",
      "websocket": "channels 4.1.0",
      "docs": "drf-yasg 1.21.7",
      "async": "dvadmin3-celery 3.1.6"
    },
    "frontend": {
      "language": "TypeScript",
      "framework": "Vue 3.4.38",
      "build_tool": "Vite 5.4.1",
      "ui_library": "Element Plus 2.8.0",
      "state_management": "Pinia 2.0.28",
      "crud": "@fast-crud/fast-crud 1.21.2",
      "router": "Vue Router 4.4.3"
    },
    "database": {
      "default": "SQLite3",
      "recommended": "MySQL 8.0+",
      "optional": "PostgreSQL"
    },
    "cache": {
      "optional": "Redis 6.2.6+"
    }
  },
  "modules": [
    {
      "name": "backend",
      "path": "backend/",
      "language": "Python",
      "framework": "Django 4.2.14",
      "description": "Django 后端服务，提供 RESTful API、WebSocket 服务",
      "entry_points": [
        "manage.py",
        "main.py",
        "application/asgi.py",
        "application/wsgi.py"
      ],
      "api_routes": "/api/",
      "has_tests": true,
      "test_files": [
        "backend/dvadmin/system/tests.py"
      ],
      "config_files": [
        "application/settings.py",
        "conf/env.example.py",
        "gunicorn_conf.py"
      ],
      "submodules": [
        "application",
        "dvadmin/system",
        "dvadmin/utils",
        "conf",
        "plugins"
      ]
    },
    {
      "name": "web",
      "path": "web/",
      "language": "TypeScript/Vue3",
      "framework": "Vue 3.4.38 + Vite 5.4.1",
      "description": "Vue3 前端应用，提供用户界面、权限控制、状态管理",
      "entry_points": [
        "src/main.ts",
        "index.html"
      ],
      "config_files": [
        "vite.config.ts",
        "tsconfig.json",
        "package.json",
        ".eslintrc.js",
        ".prettierrc.js"
      ],
      "has_tests": false,
      "build_output": "dist/",
      "env_files": [
        ".env",
        ".env.development",
        ".env.production",
        ".env.local_prod"
      ],
      "submodules": [
        "src/api",
        "src/components",
        "src/layout",
        "src/views",
        "src/stores",
        "src/router",
        "src/utils",
        "src/directive",
        "src/i18n"
      ]
    },
    {
      "name": "application",
      "path": "backend/application/",
      "language": "Python",
      "description": "Django 应用配置（URL、Settings、WSGI/ASGI）",
      "parent": "backend",
      "entry_points": [
        "settings.py",
        "urls.py",
        "asgi.py",
        "wsgi.py"
      ],
      "config_files": [
        "settings.py",
        "urls.py"
      ]
    },
    {
      "name": "system",
      "path": "backend/dvadmin/system/",
      "language": "Python",
      "description": "系统核心模块（用户/角色/权限/菜单/部门等）",
      "parent": "backend",
      "models": [
        "Users",
        "Role",
        "Dept",
        "Post",
        "Menu",
        "MenuButton",
        "MenuField",
        "Dictionary",
        "Area",
        "FileList",
        "ApiWhiteList",
        "SystemConfig",
        "LoginLog",
        "OperationLog",
        "MessageCenter"
      ],
      "views": [
        "UserViewSet",
        "RoleViewSet",
        "DeptViewSet",
        "MenuViewSet",
        "MenuButtonViewSet",
        "MenuFieldViewSet",
        "DictionaryViewSet",
        "AreaViewSet",
        "FileViewSet",
        "ApiWhiteListViewSet",
        "SystemConfigViewSet",
        "MessageCenterViewSet",
        "LoginLogViewSet",
        "OperationLogViewSet"
      ],
      "management_commands": [
        "init",
        "init_area",
        "generate_init_json"
      ]
    },
    {
      "name": "utils",
      "path": "backend/dvadmin/utils/",
      "language": "Python",
      "description": "工具类与基础组件（模型/视图集/权限/异常等）",
      "parent": "backend",
      "core_classes": [
        "CoreModel",
        "SoftDeleteModel",
        "CustomModelViewSet",
        "CustomPermission",
        "CustomPagination",
        "SuccessResponse",
        "ErrorResponse"
      ],
      "utilities": [
        "models.py",
        "viewset.py",
        "permission.py",
        "filters.py",
        "pagination.py",
        "serializers.py",
        "json_response.py",
        "exception.py",
        "middleware.py",
        "backends.py",
        "import_export_mixin.py",
        "field_permission.py",
        "swagger.py",
        "aliyunoss.py",
        "tencentcos.py"
      ]
    },
    {
      "name": "docker_env",
      "path": "docker_env/",
      "language": "Docker",
      "description": "Docker 容器配置",
      "services": [
        "django",
        "web",
        "mysql",
        "redis",
        "celery",
        "nginx"
      ],
      "config_files": [
        "docker-compose.yml",
        "django/Dockerfile",
        "web/Dockerfile",
        "mysql/conf.d/my.cnf",
        "redis/redis.conf"
      ]
    }
  ],
